{
    "version": 2,
    "builds": [
        {
            "src": "src/backend/server.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/src/backend/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/src/front/$1"
        }
    ]
}